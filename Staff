CREATE TABLE Staff (
    staff_id INT PRIMARY KEY,
    first_name VARCHAR(100) NOT NULL,
    last_name VARCHAR(100) NOT NULL,
    role VARCHAR(100)
);

INSERT INTO Staff (staff_id, first_name, last_name, role)
VALUES (1, 'Alice', 'Johnson', 'Librarian'),
	   (2, 'Bob', 'Williams', 'Assistant');

SELECT * FROM Staff
